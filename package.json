{
  "name": "do-language-support",
  "displayName": "Do Language Support",
  "description": "add .do files language support",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "do",
        "aliases": [
          "do",
          "do"
        ],
        "extensions": [
          ".do"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "do",
        "scopeName": "source.do",
        "path": "./syntaxes/do.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "url": "https://github.com/Jibaru/vscode-do-language-support",
    "type": "git"
  },
  "keywords": [
    "syntax highlighting",
    "do files",
    "programming",
    "http requests"
  ],
  "author": {
    "name": "Jibaru"
  },
  "publisher": "jibaru",
  "icon": "logo.png"
}
