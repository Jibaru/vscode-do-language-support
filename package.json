{
  "name": "do-language-support",
  "displayName": "do language support",
  "description": "add do language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "do",
      "aliases": ["do", "do"],
      "extensions": [".do"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "do",
      "scopeName": "source.do",
      "path": "./syntaxes/do.tmLanguage.json"
    }]
  }
}
