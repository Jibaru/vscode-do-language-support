{
  "fileTypes": ["do"],
  "name": "Do Language",
  "patterns": [
    {
      "name": "comment.line.double-slash.do",
      "match": "//.*$"
    },
    {
      "name": "string.quoted.double.do",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "variable.interpolated.do",
          "match": "\\$\\w+"
        }
      ]
    },
    {
      "name": "string.quoted.backtick.do",
      "begin": "`",
      "end": "`",
      "patterns": [
        {
          "name": "variable.interpolated.do",
          "match": "\\$\\w+"
        }
      ]
    },
    {
      "name": "keyword.control.do",
      "match": "\\b(do|let)\\b"
    },
    {
      "name": "constant.numeric.do",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "constant.language.boolean.do",
      "match": "\\b(true|false)\\b"
    },
    {
      "name": "keyword.operator.assignment.do",
      "match": "="
    },
    {
      "name": "punctuation.terminator.do",
      "match": ";"
    },
    {
      "name": "punctuation.definition.block.do",
      "match": "[{}]"
    },
    {
      "name": "support.function.do",
      "match": "\\b\\w+\\s*\\("
    },
    {
      "name": "entity.name.tag.do",
      "match": "\\b(method|url|params|query|headers|body)\\b"
    }
  ],
  "repository": {},
  "scopeName": "source.do"
}
